package edu.uwm.elsevier.analysis;

/**
 * @author qing
 *
 */
public class ArticleNodeStatistics {
	
	protected int articleId;
	protected int incites =0;
	protected int outcites=0;
	protected int numOfAuthors =0;
	public int getArticleId() {
		return articleId;
	}
	public void setArticleId(int articleId) {
		this.articleId = articleId;
	}
	public int getIncites() {
		return incites;
	}
	public void setIncites(int incites) {
		this.incites = incites;
	}
	public int getOutcites() {
		return outcites;
	}
	public void setOutcites(int outcites) {
		this.outcites = outcites;
	}
	public int getNumOfAuthors() {
		return numOfAuthors;
	}
	public void setNumOfAuthors(int numOfAuthors) {
		this.numOfAuthors = numOfAuthors;
	}

	
}
